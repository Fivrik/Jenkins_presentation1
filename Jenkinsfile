pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'I just built this stage'
      }
    }

    stage('Test') {
      steps {
        echo 'I just tested this stage'
      }
    }

    stage('Deploy') {
      steps {
        echo 'I just deployed this stage'
      }
    }

  }
}